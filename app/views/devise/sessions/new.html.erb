<div class="row" rel="page" style="padding: 10px;">
	<div class="main-content" style="width:660px;height:50px">
		<div class="page-header">
			<h2>欢迎登录 <small>胜因营销管理</small></h2>
        </div>
        <div id="sel_mes" class="stream-content" style="padding: 10px;">
        	<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
			<div style="padding: 10px">
				<%= f.label :email %>
				<%= f.email_field :email %>
			</div>
			<div style="padding: 10px">
				<%= f.label :password %>
				<%= f.password_field :password %>
			</div>
			<% if devise_mapping.rememberable? %>
			<div style="padding: 10px">
				<%= f.check_box :remember_me %>
				<%= f.label :remember_me %>
			</div>
			<% end %>
			<br />
			<div style="padding: 10px;">
				<%= render :partial => "devise/shared/links" %>
			</div>
			<div style="padding: 10px"><%= f.submit "登入" %></div>
			<% end %>
        </div>
	</div>
</div>

